#include "COTS/LIB/Bit_Math.h"
#include "COTS/LIB/STD_Types.h"
#include "COTS/MCAL/DIO/DIO_interface.h"
#include "COTS/HAL/LCD/LCD_interface.h"
#include "avr/delay.h"

int main(){

	LCD_voidInitialize();

	LCD_voidWriteData('A');
	LCD_voidWriteData('A');
	LCD_voidWriteData('B');
	while(1);
	return 0;
}
